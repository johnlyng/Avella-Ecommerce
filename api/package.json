{
  "name": "avella-api",
  "version": "1.0.0",
  "description": "Avella Ecommerce REST API",
  "main": "server.js",
  "scripts": {
    "start": "node -r ts-node/register server.js",
    "dev": "concurrently \"npm run studio\" \"nodemon --exec node -r ts-node/register server.js\"",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "studio": "drizzle-kit studio --host 0.0.0.0"
  },
  "keywords": [
    "ecommerce",
    "api",
    "rest",
    "express",
    "postgresql"
  ],
  "author": "Avella Team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.3.1",
    "drizzle-orm": "^0.45.1",
    "express": "^5.0.0",
    "express-validator": "^7.0.1",
    "http-proxy-middleware": "^2.0.9",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.18.0",
    "postgres": "^3.4.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "yamljs": "^0.3.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "drizzle-kit": "^0.31.9",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "typescript": "~5.7.0"
  }
}
